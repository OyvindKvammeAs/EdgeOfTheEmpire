@model IEnumerable<EdgeOfTheEmpire.Models.Character>

<h2>Characters</h2>
<div class="container">
    @foreach (var character in Model)
    {
        <h2>
            <a asp-controller="Character" asp-action="Index" asp-route-characterName="@character.Name">@character.Name</a>
        </h2>
    }
</div>

<div class="container">
    <a asp-controller="Character" asp-action="AddCharacter">Add new Character</a>
</div>